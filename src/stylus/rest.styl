html,body
  background #F4F6FA

*
  padding 0
  margin 0
  color  #333
  font-size 14px
  font-family "Microsoft Yahei","Hiragino Sans GB","Helvetica Neue",Helvetica,tahoma,arial,Verdana,sans-serif,"WenQuanYi Micro Hei","\5B8B\4F53"
a
  color #333
  text-decoration  none
ul, ol, li
  list-style none
h1, h2, h3, h4, h5, h6
  font-weight normal
img
  display inline-block
  border none
em, b, i
  font-weight normal
  font-style normal
.clearfix 
  zoom 1
  &:after 
    display block
    height 0
    clear both
    content ""
.inner
  width 1200px
  margin 0 auto
