.check{border-top:1px solid #E1E1E1; margin-top:-20px;}

/*过滤*/
.check .filter{padding:10px 30px; display:flex; background:#fff;}
.filter .f1{flex-grow:1; margin-right:70px; border-radius:2px; display:inline-flex; position:relative;border:1px solid #ffb422;}
.f1>input{padding:0 10px;flex-grow:1;}
.f1>img{align-self:center; margin-right:10px;}
.f1>img:hover{cursor:pointer; opacity:0.8;}
.f1>a{display:inline-block; flex-shrink:0; width:72px;background:url(/static/images/search.png) no-repeat center center #ffb422;}
.f1>a:hover{background-color:#fea350;}
.f1.disabled{border-color:#e1e1e1;}
.f1.disabled>a{pointer-events:none;background-color:#e1e1e1;}
.f1>ul{ border:1px solid #e1e1e1; border-top:none; background:#fff; position:absolute; top:29px; left:-1px; right:71px;}
.f1>ul>li{padding 0 10px; line-height:30px; cursor:pointer;}
.f1>ul>li:hover{background:#fff3eb;}
.f1>ul>li .sp1{display:inline-block;}
.f1>ul>li .sp2{margin-left:15px;color:#999;font-size:12px;}

.filter .f2{margin-right:20px;}

.filter .f3{font-size:13px; letter-spacing:0.27px; background:#FFF3EB; border:1px solid #FFF3EB; border-radius:2px; display:inline-flex;
  align-items:center; height:30px; box-sizing:border-box; padding:0 1px;}
.f3>a{width:50px; line-height:26px; display:inline-block; text-align:center; color:#999;}
.f3>a.active{background:#FFFFFF; border-radius:2px; color:#FFB422;}

/*表头*/
.check .tb-head{user-select:none; height:50px; padding:0 32px; font-size:13px; color:#999999; letter-spacing:0.27px;}
.tb-head span{display:inline-block; line-height:50px;}

.tb-head .sortable{cursor:pointer;}
.tb-head .sortable:hover{color:#666;}
.tb-head .sortable .sort{ position:relative; margin-left:3px;}
.tb-head .sortable .sort:hover{ color:#666;}
.tb-head .sortable .sort:before{content:'';border:5px solid transparent; border-bottom-color:#999;border-top:none;
  position:absolute; right:-15px; top:50%; margin-top:-6px;}
.tb-head .sortable .sort:after{ content:''; border:5px solid transparent;border-top-color:#999;border-bottom:none;
  position:absolute; right:-15px; top:50%; margin-top:1px;}
.tb-head .sortable .sort.up:before{border-bottom-color:#ffb422;}
.tb-head .sortable .sort.down:before{border-top-color:#ffb422;}

.tb-head .drop{position:relative;}
.tb-head .drop>a{color:#999; display:inline-block; width:100%;}
.tb-head .drop>a:after{ display:inline-block; width:0; height:0; content:""; border:5px solid transparent;
  border-top:5px solid #999; border-bottom-width: 2px; margin-left:7px;}
.tb-head .drop>ul{position:absolute; margin-top:-10px; margin-left:-10px; background:#fff; border:1px solid #e1e1e1; width:100px;}
.tb-head .drop>ul>li{line-height:30px; cursor:pointer; padding:0 10px;}
.tb-head .drop>ul>li:hover{background:#fff3eb;}

/*弹出框*/
.check .pop{ padding:60px 80px; position:relative;}
.pop .inf-li{font-size:14px; letter-spacing:0.16px; line-height:20px; margin-bottom:10px;}
.pop .inf-li>label{color:#999; width:150px; display:inline-block;}
.pop .inf-li>span{color:#333; display:inline-block;}
.pop .check-img{height:180px; display:flex; align-items:center;}
.pop .check-img>img{ width:180px; height:120px; border:1px solid #EEEEEE; border-radius:2px; margin-right:20px;}
.pop .close{position:absolute; top:10px; right:10px; font-size:20px; cursor:pointer;
   display:inline-block; line-height:24px; width:24px; text-align:center;}

/*列表项*/
.check .li{ padding:0 30px; background:#fff; border:1px solid #E1E1E1; margin-bottom:10px;}

.li .booth{font-size:14px; color:#333333; letter-spacing:0.16px;}
.li .booth>div{display:inline-block; height:70px; vertical-align:top; padding:15px 0;
  box-sizing:border-box; overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}
.li .booth>div>p{line-height:20px;}
.li .booth>div>p:not(:first-of-type){margin-top:3px;}
.li .booth>div>a{color:#ffb422;cursor:pointer;}
.li .booth>div>a:hover{color:#dca02a;}

.li .division{height:1px; background:#E1E1E1;}
.li .remark{height:50px; line-height:50px; font-size:13px;color:#999999;letter-spacing:0.23px;}

