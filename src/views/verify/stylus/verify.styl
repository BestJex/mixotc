.verify{ margin-top: 40px; margin-bottom: 40px;}
.verify h2 {
  height: 30px;
  padding-left: 5px;
  line-height: 30px;
  font-size: 20px;
  font-weight: bold;
  color: #333;
  letter-spacing: 0.41px;
}
.verify h2::before{
  display: inline-block;
  position: relative;
  top: 2px;
  left: 0
  content: '';
  width: 3px;
  height: 20px;
  margin-right: 10px;
  background-color: #FFB422;
}
.main{ margin-top: 28px;}
.main::after{ clear: both; content: ''; display: block;}
